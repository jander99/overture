{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/home/user"]
    }
  },
  "projects": {
    "/home/user/projects/managed-project": {
      "mcpServers": {
        "already-managed": {
          "command": "npx",
          "args": ["-y", "mcp-server-already-managed"],
          "_overtureManaged": true
        },
        "unmanaged-server": {
          "command": "npx",
          "args": ["-y", "mcp-server-unmanaged"]
        }
      }
    }
  }
}
